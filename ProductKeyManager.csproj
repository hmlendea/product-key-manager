<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <RootNamespace>ProductKeyManager</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NuciDAL" Version="2.1.0" />
    <PackageReference Include="NuciExtensions" Version="4.0.0" />
    <PackageReference Include="NuciLog" Version="1.1.0.1" />
    <PackageReference Include="NuciLog.Core" Version="2.3.1" />
    <PackageReference Include="NuciSecurity.HMAC" Version="2.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Update="appsettings.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Data/keys.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
